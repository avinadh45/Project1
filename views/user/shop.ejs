<script>
    function applySorting() {
        const sortByPrice = document.getElementById('sortByPrice').value;
        const sortByName = document.getElementById('sortByName').value;
        const urlParams = new URLSearchParams(window.location.search);
    
        if (sortByPrice) {
            urlParams.set('sortByPrice', sortByPrice);
        } else {
            urlParams.delete('sortByPrice');
        }
    
        if (sortByName) {
            urlParams.set('sortByName', sortByName);
        } else {
            urlParams.delete('sortByName');
        }
    
        window.location.search = urlParams.toString();
    }
    </script>
    
    <script>
        function handleSearch() {
        const searchInput = document.querySelector('input[name="search"]').value;
        const categoryId = "<%= typeof categoryId !== 'undefined' ? categoryId : '' %>";
        let url = '/shop?';

        if (categoryId) {
            url += `categoryId=${categoryId}&`;
        }
        
        if (searchInput) {
            url += `search=${encodeURIComponent(searchInput)}`;
        }

        
        window.location.href = url;

        return false;
    }
    </script>
    
    

