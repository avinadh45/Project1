async function applyReferralCode() {
    const referralCode = document.getElementById('inputReferralCode').value;
    if (!referralCode) {
        Toastify({
            text: "Please enter a referral code.",
            backgroundColor: "red",
            duration: 3000
        }).showToast();
        return;
    }

    try {
        const response = await fetch('/applyReferral', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
            },
            body: JSON.stringify({ referralCode }),
        });

        const result = await response.json();
        if (result.success) {
            Toastify({
                text: result.message,
                backgroundColor: "green",
                duration: 3000
            }).showToast();
        } else {
            Toastify({
                text: result.message,
                backgroundColor: "red",
                duration: 3000
            }).showToast();
        }
    } catch (error) {
        console.error('Error applying referral code:', error);
        Toastify({
            text: "An error occurred. Please try again.",
            backgroundColor: "red",
            duration: 3000
        }).showToast();
    }
}
