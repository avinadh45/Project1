        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List </h2>
                    <p>Orders will show here.</p>
                </div>
              
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-2 col-6 col-md-3">
                        <select id="orderFilter" onchange="filterOrders()">
                            <option value="latest" <%= filter == 'latest' ? 'selected' : '' %>>Latest</option>
                            <option value="oldest" <%= filter === 'oldest' ? 'selected' : '' %>>Oldest</option>
                            <option value="Pending" <%= filter === 'Pending' ? 'selected' : '' %>>Pending</option>
                            <option value="Delivered" <%= filter === 'Delivered' ? 'selected' : '' %>>Delivered</option>
                            <option value="Cancelled" <%= filter === 'Cancelled' ? 'selected' : '' %>>Cancelled</option>
                            <option value="Confirmed" <%= filter === 'Confirmed' ? 'selected' : '' %>>Confirmed</option>
                        </div>
                        </select>
                       
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <!-- <th>#ID</th> -->
                                    <th scope="col">Name</th> 
                                    <th scope="col">Email</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% orders.forEach(order => { 
                                    const user = users.find(user => user._id.toString() === order.userId.toString());
                                %>
                                  <tr>
                                  
                                    <td><%= user ? user.name : 'Unknown User' %></td> 
                                    <td><%= user ? user.email : 'Unknown Email' %></td>
                                    <td>₹<%= order.totalprice %></td> 
                                    <td><span class="badge rounded-pill alert-warning"><%= order.status %></span></td>
                                    <td><%= new Date(order.placed).toLocaleDateString() %></td> 
                                    <td class="text-end">
                                      <a href="/admin/order-details?id=<%= order._id %>" class="btn btn-md rounded font-sm">Detail</a>
                                      
                                    </td>
                                  </tr>
                                <% }) %>
                            
                            </tbody>
                        </table>
                    </div>
                </div> 
            </div> 
            <div class="pagination-area mt-15 mb-50">
                <nav aria-label="Page navigation example">
    <ul class="pagination justify-content-start">
        <% if (currentPage > 1) { %>
            <li class="page-item">
                <a class="page-link" href="?page=<%= currentPage - 1 %>&filter=<%= filter %>">
                    <i class="material-icons md-chevron_left"></i>
                </a>
            </li>
        <% } %>

        <% for (let i = 1; i <= totalPages; i++) { %>
            <% if (i === 1 || i === totalPages || (i >= currentPage - 2 && i <= currentPage + 2)) { %>
                <li class="page-item <%= currentPage === i ? 'active' : '' %>">
                    <a class="page-link" href="?page=<%= i %>&filter=<%= filter %>"><%= ('0' + i).slice(-2) %></a>
                </li>
            <% } else if (i === currentPage - 3 || i === currentPage + 3) { %>
                <li class="page-item">
                    <span class="page-link">...</span>
                </li>
            <% } %>
        <% } %>

        <% if (currentPage < totalPages) { %>
            <li class="page-item">
                <a class="page-link" href="?page=<%= currentPage + 1 %>&filter=<%= filter %>">
                    <i class="material-icons md-chevron_right"></i>
                </a>
            </li>
        <% } %>
    </ul>
</nav>

