        const codMessageElement = document.getElementById('codMessage');

        const checkSubtotal = () => {
            const subtotal = parseFloat(subtotalElement.innerText);
            if (subtotal > 5000) {
                paymentCodElement.disabled = true;
                codMessageElement.style.display = 'block';
                paymentCodElement.parentElement.style.color = 'gray'; 
            } else {
                paymentCodElement.disabled = false;
                codMessageElement.style.display = 'none';
                paymentCodElement.parentElement.style.color = ''; 
            }
        };

        checkSubtotal(); 
    });
</script>


    
